                      <button type="submit"><i class="fa-solid fa-paper-plane"></i></button>
                    </form>
                  </div>
                </div>
              </div>
              <div class="edit__comment">
                <%if(showComments.length > 0) {%> <% showComments.forEach(comment => {%> <% if(comment._id == idComment) {%>
                <div>
                  <a href="profile.html">
                    <img src="/images/about.jpeg" class="profile-photo" alt="" />
                  </a>
                </div>
                <form action="/update/<%=comment._id%>?_method=PUT" method="POST" class="edit__form">
                  <div class="comment__content">
                    <input type="text" value="<%=comment.commentBody%>" name="commentBody" />
                  </div>
                  <!-- Accept the Change -->
                  <div class="btns">
                    <button type="submit" class="btn btn-link">
                      <i class="fa-solid fa-paper-plane"></i>
                    </button>
                    <!-- Cancle the Change -->
                    <a class="cancle__edit" href="/home.ejs">
                      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x" viewBox="0 0 16 16">
                        <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z" />
                      </svg>
                    </a>
                  </div>
                </form>
              </div>
              <%}%> <%})%> <%}%>
            </div>
          </li>
          <%}) %>
        </ul>
        <%} %>
